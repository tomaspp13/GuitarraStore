
@{
    ViewData["Title"] = "Administrador de datos";
}

<body class="bg-light">

    <div class="container mt-4">


        <div class="d-flex justify-content-between align-items-center mb-4">
            <h2 class="mb-0">Listado de Guitarras</h2>

            <div class="d-flex ms-auto gap-2">
                <button id="botonagregar" class="btn btn-primary">
                    <i class="bi bi-plus-circle"></i> Agregar
                </button>

                <form asp-controller="Usuarios" asp-action="CerrarSesion" method="post">
                    <button type="submit" class="btn btn-outline-danger d-flex align-items-center gap-2">
                        <i class="bi bi-box-arrow-right fs-5"></i>
                        <span>Cerrar Sesión</span>
                    </button>
                </form>
            </div>
        </div>


        <ul id="listado" class="list-group">
            
        </ul>

    </div>

    
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css" rel="stylesheet">

    <script type="module" src="/js/main.js"></script>
</body>

